#!groovy

@Library("workflowlibs@master") _

pipeline {

    agent none
    parameters {
        file(description: 'Ether Configuration', name: 'etherConfig')
    }

    stages {
        stage('Checkout Global Library') {
            steps {
                script{
                    globalBootstrap {
                        libraryName   = "cellsworkflowlibs"
                        libraryBranch = "master"

                        entrypointParams = [
                            type                  : "npm",
                            lint                  : false,
                            sonarQube             : false,
                            test                  : false,
                            publish               : true
                        ]
                    }
                }
            }
        }
    }
}